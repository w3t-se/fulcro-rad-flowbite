{
  "name": "fulcro-rad-flowbite",
  "version": "1.0.0",
  "description": "Fulcro rad test app.",
  "scripts": {
    "shadow:watch": "shadow-cljs watch app",
    "shadow:release": "shadow-cljs release app",
    "postcss:build": "cross-env TAILWIND_MODE=build postcss src/css/tailwind.css -o ./resources/public/css/main.css --verbose",
    "postcss:watch": "cross-env TAILWIND_MODE=watch postcss src/css/tailwind.css -o ./resources/public/css/main.css --verbose -w",
    "postcss:release": "cross-env NODE_ENV=production postcss src/css/tailwind.css -o ./resources/public/css/main.css --verbose",
    "dev": "run-p -l *:watch",
    "release": "run-s *:release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/w3t-se/fulcro-test.git"
  },
  "author": "Fulcrologic, LLC",
  "license": "MIT",
  "bugs": {},
  "dependencies": {
    "@headlessui/react": "^1.7.7",
    "@tailwindcss/forms": "^0.5.3",
    "big.js": "5.2.2",
    "create-react-class": "15.7.0",
    "cross-env": "^7.0.3",
    "flowbite": "^1.6.2",
    "flowbite-datepicker": "^1.2.2",
    "flowbite-react": "^0.3.7",
    "highlight.js": "^9.12.0",
    "intl-messageformat": "^9.3.11",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "tailwind-datepicker-react": "^1.2.3"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.12",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.20",
    "postcss-cli": "^10.1.0",
    "shadow-cljs": "^2.20.16",
    "tailwindcss": "^3.2.4"
  }
}
